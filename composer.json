{
    "name": "dormilich/ripedb-client",
    "description": "A PHP library to communicate with the RIPE NCC database.",
    "type": "library",
    "license": "LGPL-2.1",
    "homepage": "https://github.com/Dormilich/ripedb-client",
    "authors": [
        {
            "name": "Bertold von Dormilich",
            "email": "Dormilich@netscape.net",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Dormilich/ripedb-client/issues",
        "source": "https://github.com/Dormilich/ripedb-client.git"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dormilich\\WebService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/Test/"
        }
    },
    "scripts": {
        "test": "phpunit --testsuite local"
    },
    "minimum-stability": "stable"
}
